{
  "private": true,
  "name": "firestorter-example",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template index.html",
    "serve": "preact build --template index.html && preact serve",
    "dev": "preact watch --template index.html",
    "lint": "eslint src",
    "test": "mocha --recursive --timeout 10000 --compilers js:babel-register",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.13.0",
    "babel-preset-stage-2": "^6.11.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "if-env": "^1.0.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.5.3",
    "preact-cli": "^2.0.0",
    "sass-loader": "^6.0.6",
    "@storybook/react": "^3.4.10",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "babel-runtime": "^6.26.0"
  },
  "dependencies": {
    "fecha": "^2.3.1",
    "firebase": "^5.3.1",
    "firestorter": "^0.14.2",
    "highlight.js": "^9.12.0",
    "lodash.find": "^4.6.0",
    "markdown-it": "^8.4.2",
    "markdown-it-highlightjs": "^3.0.0",
    "markdown-it-prism": "^1.1.2",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.5",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.2.0",
    "reactstrap": "^4.8.0"
  }
